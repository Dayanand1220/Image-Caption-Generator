{
  "version": 2,
  "name": "image-caption-generator",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "functions": {
    "frontend/build/**": {
      "includeFiles": "frontend/build/**"
    }
  }
}